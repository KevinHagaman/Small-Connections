{
  "storage": {
    "SmallConnectionsAWS": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    }
  },
  "function": {
    "businessLambda": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "SmallConnectionsAWS",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    }
  },
  "api": {
    "businessapi": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "businessLambda",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    }
  }
}